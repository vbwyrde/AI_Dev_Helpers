[
    {
        "key": "ctrl+shift+m",
        "command": "workbench.action.terminal.newWithProfile",
        "args": {
            "profileName": "Command Prompt"
        }
    },
    {
        "key": "ctrl+shift+m",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "cd /d C:\\Users\\<UserName>\\Desktop\\ChangeMonitor && run.bat\r\n"
        },
        "when": "terminalFocus"
    }
]
